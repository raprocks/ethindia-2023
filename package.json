        "winston": "^3.10.0"
    },
    "type": "module",
    "devDependencies": {
        "@total-typescript/ts-reset": "^0.5.1",
        "@types/express": "^4.17.17",
        "@types/node": "^20.6.0",
        "@typescript-eslint/eslint-plugin": "^6.7.5",
        "eslint": "^8.49.0",
        "eslint-config-prettier": "^9.0.0",
        "express-winston": "^4.2.0",
        "nodemon": "^3.0.1",
